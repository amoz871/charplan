<FindBugsFilter>
    <Match>
        <Class name="~.*\.R\$.*"/>
        <Bug code="Nm"/>
    </Match>

    <Bug code="UwF" /> <!-- 'UwF' is the code for bugs related code with unwritten field -->

    <!-- Error Priority: Normal -->
    <Match>
        <Class name="com.uefa.venue.ui.appointments.AppointmentsAdapter$MatchesViewHolder"/>
        <Bug pattern="SF_SWITCH_FALLTHROUGH"/>
    </Match>
    <Match>
        <Class name="com.uefa.venue.ui.appointments.AppointmentsAdapter$MatchesViewHolder"/>
        <Bug pattern="NP_LOAD_OF_KNOWN_NULL_VALUE"/>
    </Match>
    <Match>
        <Class name="com.uefa.venue.ui.appointments.AppointmentsAdapter$SiteVisitViewHolder"/>
        <Bug pattern="SF_SWITCH_FALLTHROUGH"/>
    </Match>
    <Match>
        <Class name="com.uefa.venue.ui.appointments.AppointmentsAdapter$SiteVisitViewHolder"/>
        <Bug pattern="NP_LOAD_OF_KNOWN_NULL_VALUE"/>
    </Match>
</FindBugsFilter>
